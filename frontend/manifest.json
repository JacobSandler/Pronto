{
    "manifest_version": 3,
    "name": "Newz Temp",
    "version": "1.0.0",
    "icons":{
        "128" : "icon128.png"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png"
        },
        "default_title": "Press Me Poggers",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "permissions": [
        "storage"
    ]
}