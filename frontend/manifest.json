

{
    
    "manifest_version": 3,
    "name": "Pronto",
    "version": "1.0.0",
    "host permissions": [
        "https://*/",
        "https://us-central1-python-test-308204.cloudfunctions.net/",
        "http://us-central1-python-test-308204.cloudfunctions.net/"
    ],
    "icons":{
        "128" : "images/ProntoIcon128.png"
    },
    "action": {
        "default_icon": {
            "128":"images/ProntoIcon128.png",
            "48": "images/ProntoIcon(V8)48.png",
            "32": "images/ProntoIcon(V1)32.png"
        },
        "default_title": "Pronto: Click Here",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "settings/options.html",
    "permissions": [
        "storage",
        "https://*/"
    ]
}

