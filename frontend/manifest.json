

{
    
    "manifest_version": 3,
    "name": "Newz Temp",
    "version": "1.0.0",
    "host permissions": [
        "https://*/"
    ],
    "icons":{
        "128" : "icon128.png"
    },
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png"
        },
        "default_title": "Press Me Poggers",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js",
        "scripts": ["background.js"],
        "persistent": false
    },
    "options_page": "settings/options.html",
    "permissions": [
        "storage",
        "https://*/"
    ]
}

